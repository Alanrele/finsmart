# Railway-specific Environment Variables for FinSmart Frontend

# API Configuration (will be same domain in production)
VITE_API_URL=https://finsmart-production.up.railway.app

# Microsoft Graph Configuration
VITE_GRAPH_CLIENT_ID=YOUR_GRAPH_CLIENT_ID
VITE_GRAPH_TENANT_ID=common
VITE_GRAPH_REDIRECT_URI=https://finsmart-production.up.railway.app/auth/ms-callback

# Application Configuration
VITE_APP_NAME=FinSmart
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# Feature Flags for Railway
VITE_ENABLE_DEMO_MODE=false
VITE_ENABLE_DEBUG=false
VITE_DISABLE_SOCKET=false